@startuml sequence_diagram
hide footbox
autonumber

actor Mahasiswa
boundary Beranda
boundary KHS
control KHSController
control Controller
entity KHSModel

Beranda -> Mahasiswa : Menampilkan halaman Beranda

activate Beranda

Mahasiswa -> Beranda : Menekan menu KHS

Beranda -> KHSController : index()

activate KHSController

KHSController -> Controller : renderPage()

deactivate KHSController

activate Controller

Controller --> KHS : view()

deactivate Beranda

activate KHS

deactivate Controller

KHS -> Mahasiswa : Menampilkan halaman KHS

KHS -> KHSController  : all()

activate KHSController
@startuml sequence_diagram
hide footbox
autonumber

actor Mahasiswa
boundary Beranda
boundary Kelas
control KelasController
control Controller
entity KelasModel

Beranda -> Mahasiswa : Menampilkan halaman Beranda

activate Beranda

Mahasiswa -> Beranda : Menekan menu Kelas

Beranda -> KelasController : index()

activate KelasController

KelasController -> Controller : renderPage()

deactivate KelasController

activate Controller

Controller --> Kelas : view()

deactivate Beranda

activate Kelas

deactivate Controller

Kelas -> Mahasiswa : Menampilkan halaman KHS

Kelas -> KelasController  : all()

activate KelasController

KelasController -> KelasModel : with()->get()

activate KelasModel

KelasModel -> KelasController : data

deactivate KelasModel

KelasController -> Kelas : response(data)

deactivate KelasController

Kelas -> Mahasiswa : Merender data Nilai

@enduml
