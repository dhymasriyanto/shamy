@startuml sequence_diagram
hide footbox
autonumber

actor Mahasiswa
boundary Beranda
boundary Mengajar
control MengajarController
control Controller
entity MengajarModel

Beranda -> Mahasiswa : Menampilkan halaman Beranda

activate Beranda

Mahasiswa -> Beranda : Menekan menu Mengajar

Beranda -> MengajarController : index()

activate MengajarController

MengajarController -> Controller : renderPage()

deactivate MengajarController

activate Controller

Controller --> Mengajar : view()

deactivate Beranda

activate Mengajar

deactivate Controller

Mengajar -> Mahasiswa : Menampilkan halaman Mengajar

Mengajar -> MengajarController  : all()

activate MengajarController
@startuml sequence_diagram
hide footbox
autonumber

actor Mahasiswa
boundary Beranda
boundary Mengajar
control MengajarController
control Controller
entity MengajarModel

Beranda -> Mahasiswa : Menampilkan halaman Beranda

activate Beranda

Mahasiswa -> Beranda : Menekan menu Pengajaran

Beranda -> MengajarController : index()

activate MengajarController

MengajarController -> Controller : renderPage()

deactivate MengajarController

activate Controller

Controller --> Mengajar : view()

deactivate Beranda

activate Mengajar

deactivate Controller

Mengajar -> Mahasiswa : Menampilkan halaman Mengajar

Mengajar -> MengajarController  : all()

activate MengajarController

MengajarController -> MengajarModel : with()->get()

activate MengajarModel

MengajarModel -> MengajarController : data

deactivate MengajarModel

MengajarController -> Mengajar : response(data)

deactivate MengajarController

Mengajar -> Mahasiswa : Merender data Nilai

@enduml
